{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "material-icon-theme.activeIconPack": "react_redux",
  "cSpell.words": [
    "commitlint",
    "reduxjs",
    "tailwindcss",
    "unoptimized",
    "viewports"
  ]
}
